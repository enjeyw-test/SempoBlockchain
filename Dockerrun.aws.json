{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:server",
      "essential": true,
      "memoryReservation": 256,
      "cpu": 1,
      "links": ["pgbouncer:pgbouncer"],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "APP"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "pgbouncer",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:pgbouncer",
      "essential": true,
      "memory": 32,
      "cpu": 1,
      "environment": [],
      "portMappings": [
        {
          "hostPort": 6543,
          "containerPort": 6543
        }
      ],
      "links": []
    },
    {
      "name": "proxy",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:proxy",
      "essential": true,
      "memory": 32,
      "cpu": 1,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": [
        "app:app"
      ]
    },
    {
      "name": "high_pri_eth_worker",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:eth_worker",
      "essential": true,
      "memory": 128,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "HIGH_PRIORITY_WORKER"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "low_pri_eth_worker",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:eth_worker",
      "essential": true,
      "memory": 128,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "LOW_PRIORITY_WORKER"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "beat",
      "image": "290492953667.dkr.ecr.ap-southeast-2.amazonaws.com/blockchaindemo:worker",
      "essential": false,
      "memory": 128,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "BEAT"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    }
  ]
}